<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>CP0105 - Veteran Marriage Validation</name>
  <uuid>80f869ab-d37f-4f01-bc34-2bbc38cf960d</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if
	the current marriage of 'the Veteran' is present and
    the latest previous marriage of 'the Veteran' is present and
	the start date of the current marriage of 'the Veteran' is before the end date of the latest previous marriage of 'the Veteran' 
then
	add "Auto Dependency Processing Reject Reason - Submitted Veteran's prior marriage date(s) do not terminate before date of current marriage. Please review." to the list of Exceptions ;]]></definition>
  <effectiveDate xsi:nil="true"/>
  <expirationDate xsi:nil="true"/>
</ilog.rules.studio.model.brl:ActionRule>
