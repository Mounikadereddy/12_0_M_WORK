<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>CP0103 - Is Spouse Existing On Award</name>
  <uuid>d38c61f3-ec2a-439e-9067-8ea509c6e6cd</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if
	the end date of the marriage of 'the Spouse' is not present and
	'the Spouse' is existing on current award 
	
then 
	add "Auto Dependency Processing Reject Reason - Spouse is already existing on award. Please review." to the list of Exceptions ;
	set 'exception generated' to true ;
	]]></definition>
  <effectiveDate xsi:nil="true"/>
  <expirationDate xsi:nil="true"/>
</ilog.rules.studio.model.brl:ActionRule>
